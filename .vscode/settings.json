{
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "idf.espIdfPath": "${env:HOME}/esp/v6.0/esp-idf",
  "idf.pythonInstallPath": "/usr/local/bin/python3",
  "idf.openOcdConfigs": [
    "board/esp32s3-builtin.cfg"
  ],
  "idf.port": "/dev/tty.usbmodem1101",
  "idf.toolsPath": "${env:HOME}",
  "idf.customExtraVars": {
    "IDF_TARGET": "esp32s3"
  },
  "clangd.path": "${env:HOME}/tools/esp-clang/esp-20.1.1_20250829/esp-clang/bin/clangd",
  "clangd.arguments": [
    "--background-index",
    "--query-driver=${env:HOME}/tools/xtensa-esp-elf/esp-15.2.0_20251204/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
    "--compile-commands-dir=${workspaceFolder}/build"
  ],
  "idf.flashType": "JTAG",
  "files.associations": {
    "array": "c",
    "string": "c",
    "string_view": "c",
    "span": "c",
    "esp_log.h": "c",
    "cstdlib": "c"
  }
}
